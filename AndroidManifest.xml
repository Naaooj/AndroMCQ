<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lu.ctg.db29"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/checkbox"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="lu.ctg.mcq.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="lu.ctg.mcq.McqActivity"
            android:label="@string/title_activity_mcq" >
            <intent-filter>
                <action android:name="lu.ctg.db29.MCQ_ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="lu.ctg.mcq.McqResultActivity"
            android:label="@string/title_activity_mcq_result" >
        </activity>
        <activity
            android:name="lu.ctg.mcq.McqDetailActivity"
            android:label="@string/title_activity_mcq_detail" >
        </activity>
    </application>

</manifest>
